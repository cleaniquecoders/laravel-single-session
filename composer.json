{
    "name": "cleaniquecoders/laravel-single-session",
    "description": "Enable only single session of user logged in to your Laravel application.",
    "keywords": ["package", "laravel", "single", "session"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\LaravelSingleSession\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\LaravelSingleSession\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^5.5|^5.6|^5.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "phpunit/phpunit": "^6.5|^7.0",
        "orchestra/testbench": "3.5.*|3.6.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\LaravelSingleSession\\LaravelSingleSessionServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}